    "gatsby-image": "^2.4.9",
    "gatsby-plugin-canonical-urls": "^3.3.0",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^2.7.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-mdx": "^1.2.18",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-preconnect": "^1.1.54",
